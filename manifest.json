{
    "name": "Privacy Extension for your personal chat list",
    "description": "Hi! This unofficial extension hides (and reveals) chat list content on demand - for more privacy.",
    "version": "0.0.1",
    "icons": {
    },
    "manifest_version": 3,
    "background": {},
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "Privacy -\nClick for options!"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/*"
            ],
            "js": [
                "script/script.js"
            ],
            "css": [
                "/script/script.css",
                "./script/blur-conversations.css"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ]
}